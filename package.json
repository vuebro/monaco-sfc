{
  "name": "@vuebro/monaco-sfc",
  "version": "1.0.17",
  "description": "A Monaco Editor language server for Vue SFC",
  "keywords": [
    "vue",
    "vuejs",
    "vue3",
    "monaco"
  ],
  "homepage": "https://github.com/vuebro/monaco-sfc#readme",
  "bugs": {
    "url": "https://github.com/vuebro/monaco-sfc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuebro/monaco-sfc.git"
  },
  "license": "AGPL-3.0-only",
  "author": {
    "name": "Jerry Bruwes",
    "email": "jbruwes@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build": "vue-tsc && vite build",
    "lint": "eslint . --ignore-pattern 'src/language-configs.ts'"
  },
  "dependencies": {
    "@remote-dom/polyfill": "^1.5.1",
    "@volar/jsdelivr": "^2.4.23",
    "@volar/monaco": "^2.4.23",
    "@vue/language-core": "^3.1.1",
    "@vue/language-service": "^3.1.1",
    "@vue/typescript-plugin": "^3.1.1",
    "monaco-editor-core": "^0.52.2",
    "typescript": "^5.9.3",
    "volar-service-typescript": "^0.0.65",
    "vscode-uri": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.4.0",
    "@prettier/plugin-pug": "^3.4.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@tsconfig/strictest": "^2.0.6",
    "@types/node": "^24.8.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/language-plugin-pug": "^3.1.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-de-morgan": "^2.0.0",
    "eslint-plugin-depend": "^1.3.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^61.1.4",
    "eslint-plugin-package-json": "^0.57.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-vue": "^10.5.1",
    "jiti": "^2.6.1",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tsc-alias": "^1.8.16",
    "vite": "^7.1.10",
    "vite-plugin-static-copy": "^3.1.4",
    "vue-tsc": "^3.1.1"
  }
}
