{
  "name": "@vuebro/monaco-sfc",
  "version": "1.0.25",
  "description": "A Monaco Editor language server for Vue SFC",
  "keywords": [
    "vue",
    "vuejs",
    "vue3",
    "monaco"
  ],
  "homepage": "https://github.com/vuebro/monaco-sfc#readme",
  "bugs": {
    "url": "https://github.com/vuebro/monaco-sfc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuebro/monaco-sfc.git"
  },
  "license": "AGPL-3.0-only",
  "author": {
    "name": "Jerry Bruwes",
    "email": "jbruwes@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint . --ignore-pattern 'src/language-configs.ts'"
  },
  "prettier": "@vuebro/configs/prettierrc",
  "dependencies": {
    "@remote-dom/polyfill": "^1.5.1",
    "@volar/jsdelivr": "^2.4.23",
    "@volar/monaco": "^2.4.23",
    "@vue/language-core": "^3.1.2",
    "@vue/language-service": "^3.1.2",
    "@vue/typescript-plugin": "^3.1.2",
    "monaco-editor-core": "^0.52.2",
    "typescript": "^5.9.3",
    "volar-service-typescript": "^0.0.66",
    "vscode-uri": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@vuebro/configs": "^1.1.54",
    "eslint": "^9.39.0",
    "vite": "^7.1.12"
  }
}
